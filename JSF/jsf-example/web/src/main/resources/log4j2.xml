<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info">

	<Appenders>
		<Console name="STDOUT" target="SYSTEM_OUT">
			<PatternLayout pattern="%5p %c{1} - %m%n" />
		</Console>
	</Appenders>

	<Loggers>

		<Root level="info">
			<AppenderRef ref="STDOUT" />
		</Root>
	</Loggers>

</Configuration>